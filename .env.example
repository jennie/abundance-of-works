# Baserow Configuration
# Get your API token from your Baserow account settings
BASEROW_API_TOKEN=your_baserow_api_token_here

# Optional: If using self-hosted Baserow, specify the API URL
BASEROW_API_URL=https://api.baserow.io

# Table IDs from your Baserow database
# You can find these in the Baserow URL when viewing each table
BASEROW_WORKS_TABLE_ID=your_works_table_id
BASEROW_CREATORS_TABLE_ID=your_creators_table_id
BASEROW_PRODUCERS_TABLE_ID=your_producers_table_id
BASEROW_PUBLISHERS_TABLE_ID=your_publishers_table_id
BASEROW_TAGS_TABLE_ID=your_tags_table_id
BASEROW_TAG_TYPES_TABLE_ID=your_tag_types_table_id
BASEROW_YEARS_TABLE_ID=your_years_table_id